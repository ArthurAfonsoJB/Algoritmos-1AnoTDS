programa
{
	
	funcao inicio()
	{
		inteiro dia_atual = 0

		escreva("Qual é o dia atual? \n")
		
		leia(dia_atual)

		inteiro oi = 365 - dia_atual

		escreva("Sabe quantos dias faltam para acabar o ano? \n")
		escreva(oi)
		
	}
}
